<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net7.0</TargetFramework>
    <NoWarn>CS0618, SKEXP0050, SKEXP0020</NoWarn>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="iTextSharp" Version="5.5.13.4" />
    <PackageReference Include="LangChain" Version="0.15.0" />
    <PackageReference Include="LangChain.Splitters.CSharp" Version="0.15.2" />
    <PackageReference Include="Microsoft.Extensions.Configuration.Json" Version="8.0.1" />
    <PackageReference Include="Microsoft.SemanticKernel" Version="1.24.1" />
    <PackageReference Include="Microsoft.SemanticKernel.Abstractions" Version="1.24.1" />
    <PackageReference Include="Microsoft.SemanticKernel.Connectors.Chroma" Version="1.24.1-alpha" />
    <PackageReference Include="Microsoft.SemanticKernel.Plugins.Memory" Version="1.24.1-alpha" />
    <PackageReference Include="OllamaSharp" Version="3.0.15" />
  </ItemGroup>

</Project>
